{
  "name": "node-ts-fastify-template",
  "version": "0.0.0",
  "description": "Immediately clonable web server template with Node.js + Typescript + Fastify",
  "type": "module",
  "engines": {
    "node": ">=22.9.0",
    "npm": ">=10.8.0"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "rm -rf dist && tsx build.ts",
    "start": "npm run build && node -r dotenv/config dist/index.cjs"
