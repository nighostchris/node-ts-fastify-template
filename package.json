  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.4",
    "esbuild": "^0.21.5",
    "esbuild-plugin-pino": "^2.2.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "nodemon": "^3.1.7",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "tinyglobby": "^0.2.6",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  },
  "dependencies": {
    "@fastify/swagger": "^9.1.0",
    "@fastify/swagger-ui": "^5.0.1",
